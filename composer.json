{
    "name": "shopsys/backend-api",
    "type": "library",
    "description": "Shopsys Framework API support",
    "keywords": ["api", "Shopsys Framework", "SSFW"],
    "license": "proprietary",
    "authors": [
        {
            "name": "Shopsys",
            "homepage": "https://www.shopsys.com/"
        }
    ],
    "autoload": {
        "psr-4": {
            "Shopsys\\BackendApiBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\BackendApiBundle\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "require": {
        "php": "^7.2",
        "ext-json": "*",
        "ext-pdo": "*",
        "doctrine/dbal": "^2.7",
        "friendsofsymfony/rest-bundle": "^2.5",
        "jms/serializer-bundle": "^2.4",
        "ramsey/uuid": "^3.8",
        "shopsys/form-types-bundle": "dev-tp-symfony4 as dev-master",
        "shopsys/framework": "dev-tp-symfony4 as dev-master",
        "shopsys/migrations": "dev-tp-symfony4 as dev-master",
        "shopsys/plugin-interface": "dev-tp-symfony4 as dev-master",
        "symfony/config": "^4.4",
        "symfony/dependency-injection": "^4.4",
        "symfony/http-foundation": "^4.4",
        "symfony/http-kernel": "^4.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "shopsys/coding-standards": "dev-tp-symfony4 as dev-master"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "9.0.x-dev"
        }
    }
}
